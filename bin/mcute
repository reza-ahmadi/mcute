#!/bin/bash

set -e

DIR=`dirname $0`

CONF_FILE=$2

if ["${CONF_FILE}" == ""]; then
CONF_FILE="mcute.conf"
fi

#mcute.conf
CONF=${DIR}/${CONF_FILE}

echo "copying generated branch files from /home/vagrant/mcute/tmp/branches_t* in the current directory"
cp /home/vagrant/mcute/tmp/branches_t* .

echo "CONF is ${CONF} "

#echo ${CONF}
echo "Executing $1 with ${CONF} parameters"

#executing
./$1 -u ${CONF}
